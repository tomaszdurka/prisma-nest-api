{
  "name": "prisma-nest-api",
  "version": "1.1.0",
  "description": "A library to help set up NestJS and Prisma projects",
  "main": "dist/generator/index.js",
  "types": "dist/generator/index.d.ts",
  "bin": {
    "prisma-nest-api": "dist/generator/index.js"
  },
  "exports": {
    ".": {
      "require": "./dist/generator/index.js"
    }
  },
  "files": [
    "src",
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "build:generator": "tsc --project tsconfig.generator.json",
    "build:all": "npm run build && npm run build:generator",
    "prepare": "npm run build:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nestjs",
    "prisma",
    "typescript",
    "orm",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.16",
    "@nestjs/swagger": "^7.2.0",
    "@prisma/client": "^6.0.0",
    "@prisma/generator-helper": "^6.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "prisma": "^6.6.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@prisma/client": "^6.0.0"
  }
}
